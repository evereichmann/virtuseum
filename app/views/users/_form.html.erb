<%= form_for @user do |f| %>
<%= f.label :username %>
<%= f.text_field :username %><br/><br/>
<%= f.label :password %>
<%= f.text_field :password %><br/><br/>
<%= f.label :name_first %>
<%= f.text_field :name_first %><br/><br/>
<%= f.label :name_last %>
<%= f.text_field :name_last %><br/><br/>
<%= f.label :location %>
<%= f.text_field :location %><br/><br/>
<%= f.label :picture %>
<%= f.text_field :picture %><br/><br/>
<%= f.submit %>
<% end %>