<%= image_tag(@user.picture)%>
<h1><%= @user.name_first %> <%= @user.name_last %></h1>
<h3><%= @user.location %>
<h3>My Collections</h3>
<%= button_to "Edit Profile", {:action => "edit"}, :method => "get" %>
<%= button_to "Delete Profile", user_path(@user), method: => "delete" , data: { confirm: "Are you sure? It will be gone forever!" }%>