<%= form_for @collection do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :user_id %>
<%= f.collection_select :user_id, @user, :id, :username %>
    <br/>
    <%= f.fields_for :item, Item.new do |builder| %>
        <%= render 'item_form', f:builder%>
        <br/>
    <% end%>
<%= f.submit %>
<% end %>
