<h1>Edit Collection</h1>
<br/>
User: <%= @current_user.username %>
<br/>
<%= form_for @collection do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
   <br/><br/>
    <%= f.fields_for :items do |builder| %>
        <%= render 'item_form', f:builder%>
    <% end%>
    <br/>
<%= f.submit %>
<% end %>